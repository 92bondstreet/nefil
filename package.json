{
  "name": "nefil",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "a11y-react-emoji": "^1.0.0",
    "b64-to-blob": "^1.2.19",
    "chokidar": "^3.0.0",
    "classnames": "^2.2.6",
    "electron-is-dev": "^1.1.0",
    "is-electron": "^2.2.0",
    "mime": "^2.4.2",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.4",
    "react-scripts": "3.0.1",
    "typeface-roboto": "^0.0.54",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "ELECTRON_PACK=true electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.10",
    "@rescripts/rescript-env": "^0.0.10",
    "concurrently": "^4.1.0",
    "electron": "^5.0.1",
    "electron-builder": "^20.40.2",
    "fetch-mock": "^7.3.3",
    "jest-dom": "^3.2.1",
    "node-fetch": "^2.5.0",
    "react-testing-library": "^7.0.0",
    "typescript": "^3.4.5",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/@uik/**/*.js",
      "!src/index.js",
      "!src/serviceWorker.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "build": {
    "appId": "com.lifen.app",
    "files": [
      "build/**/*"
    ]
  }
}
