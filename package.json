  "private": true,
  "dependencies": {
    "a11y-react-emoji": "^1.0.0",
    "classnames": "^2.2.6",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.4",
    "react-scripts": "3.0.0",
    "typeface-roboto": "^0.0.54",
